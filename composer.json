{
    "name": "mikailfaruqali/backup",
    "description": "A Laravel package to generate database backups with optional ZIP encryption and password protection. Designed for simplicity, performance, and route-based access.",
    "keywords": [
        "laravel",
        "backup",
        "database",
        "zip",
        "encryption",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0",
        "spatie/db-dumper": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22 || ^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\Backup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\Backup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
